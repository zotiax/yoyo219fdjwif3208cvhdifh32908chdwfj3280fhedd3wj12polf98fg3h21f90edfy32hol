    })
    
    local UserIconBorder = createInstance("UIStroke", {
        Parent = UserIcon,
        Color = BORDER_COLOR,
        Thickness = 1
    })
    
    local UserNameLabel = createInstance("TextLabel", {
        Name = "UserNameLabel",
        Parent = UserInfoSection,
        BackgroundTransparency = 1,
