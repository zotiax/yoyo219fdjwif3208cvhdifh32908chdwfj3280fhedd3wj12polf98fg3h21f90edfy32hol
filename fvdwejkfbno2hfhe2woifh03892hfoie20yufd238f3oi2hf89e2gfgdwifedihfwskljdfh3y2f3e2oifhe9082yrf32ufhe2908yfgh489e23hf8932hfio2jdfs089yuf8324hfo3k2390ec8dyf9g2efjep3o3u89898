    local StatusLabel = createInstance("TextLabel", {
        Name = "StatusLabel",
        Parent = StatusSection,
        BackgroundTransparency = 1,
        Position = UDim2.new(0, 8, 0, 0),
        Size = UDim2.new(1, -16, 1, 0),
        Font = Enum.Font.Code,
